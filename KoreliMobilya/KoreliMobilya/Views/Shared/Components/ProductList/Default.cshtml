
@{
    ViewData["Title"] = "Default";
}



@model List<ProductListComponentViewModel>

<table class="table table-dark table-striped-columns">
    <thead>
        <tr>
            <th>İsim</th>
            <th>Resim1</th>
            <th>Resim2</th>
            <th>Resim3</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
           
            
                    <tr>
                <td>@item.Name</td>
    
                <td><img style="width:100px; height:100px" src="/Images/@(item.ImagePath)" /></td>
                <td><img style="width:100px; height:100px" src="/Images/@(item.ImagePath2)" /></td>
                <td><img style="width:100px; height:100px" src="/Images/@(item.ImagePath3)" /></td>
            </tr>
            
        
        }
    </tbody>
</table>

